<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>Model.Registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ingresar</servlet-name>
        <servlet-class>Model.Ingresar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registro_IMC</servlet-name>
        <servlet-class>Model.Registro_IMC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ingresar_IMC</servlet-name>
        <servlet-class>Model.Ingresar_IMC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ingresar</servlet-name>
        <url-pattern>/Ingresar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro_IMC</servlet-name>
        <url-pattern>/Registro_IMC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ingresar_IMC</servlet-name>
        <url-pattern>/Ingresar_IMC</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
